{
  "expo": {
    "name": "safeye-app",
    "slug": "safeye-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/imgs/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/imgs/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/imgs/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.norbory.safeyeapp",
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/imgs/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "88cab465-f55d-4dee-9a2c-97b5972f5ff3"
      }
    }
  }
}
